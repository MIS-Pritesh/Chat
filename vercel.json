{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "index.html"
    },
    {
      "src": "/script.js",
      "dest": "script.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ]
}
